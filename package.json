    "email": "sashapop101@gmail.com"
  },

  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "eslint-config-astrohelm": "^1.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
